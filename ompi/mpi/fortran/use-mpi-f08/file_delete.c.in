PROTOTYPE VOID file_delete(CHAR_ARRAY filename, INFO info)
{
    MPI_Info c_info;
    int c_ierr;

    c_info = PMPI_Info_f2c(*info);

    c_ierr = @INNER_CALL@(filename, c_info);
    if (NULL != ierr) *ierr = OMPI_INT_2_FINT(c_ierr);
}
