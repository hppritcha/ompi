PROTOTYPE VOID file_seek_shared(FILE fh, OFFSET offset,
			        INT whence)
{
    int c_ierr;
    MPI_File c_fh = PMPI_File_f2c(*fh);

    c_ierr = @INNER_CALL@(c_fh, (MPI_Offset) *offset,
                          OMPI_FINT_2_INT(*whence));
    if (NULL != ierr) *ierr = OMPI_INT_2_FINT(c_ierr);
}
