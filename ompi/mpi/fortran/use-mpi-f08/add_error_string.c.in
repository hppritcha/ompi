PROTOTYPE void add_error_string(INT errorcode, CHAR_ARRAY string)
{
    int ierr_c;

    ierr_c = @INNER_CALL@(OMPI_FINT_2_INT(*errorcode), string);
    if (NULL != ierr) *ierr = OMPI_INT_2_FINT(ierr_c);
}
