PROTOTYPE VOID win_set_name(WIN win, CHAR_ARRAY win_name)
{
    int c_ierr;
    MPI_Win c_win;

    c_win = PMPI_Win_f2c(*win);

    /* Call the C function */

    c_ierr = @INNER_CALL@(c_win, win_name);
    if (NULL != ierr) *ierr = OMPI_INT_2_FINT(c_ierr);
}
