PROTOTYPE VOID pready_range(INT partition_low, INT partition_high, REQUEST request)
{
   int c_ierr;
   MPI_Request c_req = PMPI_Request_f2c(*request);

   c_ierr = @INNER_CALL@(OMPI_FINT_2_INT(*partition_low), OMPI_FINT_2_INT(*partition_high), c_req);
   if (NULL != ierr) *ierr = OMPI_INT_2_FINT(c_ierr);
   if (MPI_SUCCESS == c_ierr) {
      *request = PMPI_Request_c2f(c_req);
   }
}
