PROTOTYPE VOID comm_delete_attr(COMM comm, INT comm_keyval)
{
    int c_ierr;
    MPI_Comm c_comm;

    c_comm = PMPI_Comm_f2c(*comm);

    c_ierr = @INNER_CALL@(c_comm, OMPI_FINT_2_INT(*comm_keyval));
    if (NULL != ierr) *ierr = OMPI_INT_2_FINT(c_ierr);
}
