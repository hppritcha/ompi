PROTOTYPE VOID win_free_keyval(INT_INOUT win_keyval)
{
    int c_ierr;
    int c_win_keyval;

    c_win_keyval = OMPI_FINT_2_INT(*win_keyval);

    c_ierr = @INNER_CALL@(&c_win_keyval);
    if (NULL != ierr) *ierr = OMPI_INT_2_FINT(c_ierr);

    if (MPI_SUCCESS == c_ierr) {
        *win_keyval = OMPI_INT_2_FINT(c_win_keyval);
    }
}
