PROTOTYPE VOID session_get_num_psets(SESSION session, INFO info, INT_OUT npset_names)
{
    int c_ierr;
    MPI_Session c_session;
    int c_npset_names;

    c_session = PMPI_Session_f2c(*session);

    c_ierr = @INNER_CALL@(c_session, MPI_INFO_NULL, &c_npset_names);
    if (NULL != ierr) *ierr = OMPI_INT_2_FINT(c_ierr);

    if (MPI_SUCCESS == c_ierr) {
        *npset_names = OMPI_INT_2_FINT(c_npset_names);
    }
}
